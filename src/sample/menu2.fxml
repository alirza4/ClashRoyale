<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MenuController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <ImageView fitHeight="400.0" fitWidth="640.0">
               <image>
                  <Image url="@../image/4.jpg" />
               </image>
            </ImageView>
            <Button fx:id="BattleDeck" layoutX="487.0" layoutY="72.0" mnemonicParsing="false" onAction="#ClickButton" prefHeight="26.0" prefWidth="138.0" text="Battle Deck" />
            <Button fx:id="TrainingCamp" layoutX="487.0" layoutY="174.0" mnemonicParsing="false" onAction="#ClickButton" prefHeight="26.0" prefWidth="138.0" text="Training Camp" />
            <Button fx:id="BattleHistory" layoutX="487.0" layoutY="119.0" mnemonicParsing="false" onAction="#ClickButton" prefHeight="26.0" prefWidth="138.0" text="Battle History" />
            <Button fx:id="Profile" layoutX="487.0" layoutY="24.0" mnemonicParsing="false" onAction="#ClickButton" prefHeight="26.0" prefWidth="138.0" text="Profile" />
      </children>
    </AnchorPane>
  </children>
</VBox>
